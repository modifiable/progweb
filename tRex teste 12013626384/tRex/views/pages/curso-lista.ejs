<h1 class="display-4">Cursos</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>id</th>
            <th>nome</th>
            <th>sigla</th>
            <th>descricao</th>
        </tr>
    </thead>
    <tbody>
        <% cursos.forEach(function(curso){ %>
            <tr>
                <th scope="row"><%= curso.id %></th>
                <td><%= curso.nome %></td>
                <td><%= curso.sigla %></td>
                <td><%= curso.descricao %></td>
            </tr>
        <% }) %>
    </tbody>
</table>